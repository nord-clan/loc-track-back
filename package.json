{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "loc-track-back",
  "description": "loc-track-back",
  "version": "1.0.0",
  "source": "src/index.ts",
  "main": "./dist/index.js",
  "umd:main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "author": {
    "name": "Ivan Kornilov",
    "email": "ivan.kornilov@nordclan.com",
    "url": "https://t.me/xsolncu"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "backend"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.16.0",
    "npm": "npm install -g pnpm",
    "pnpm": ">=8"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --pretty --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "npm run prisma:generate && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "lint": "eslint --fix \"./**/*.{ts,tsx}\"",
    "lint:staged": "lint-staged --debug",
    "format": "prettier -w \"./**/*.{ts,tsx,json,scss,md}\"",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prepare": "husky install .husky",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@pothos/core": "^3.29.0",
    "@pothos/plugin-errors": "^3.11.0",
    "@pothos/plugin-prisma": "^3.47.2",
    "@pothos/plugin-relay": "^3.39.0",
    "@pothos/plugin-tracing": "^0.5.7",
    "@prisma/client": "^4.12.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "graphql-yoga": "^3.9.0",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "commit-message-validator": "^1.0.2",
    "crypto": "^1.0.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.+(js|ts)": [
      "eslint --fix"
    ],
    "src/**/*.+(js|ts|json|scss|md)": [
      "prettier --write",
      "prettier -c"
    ]
  },
  "config": {
    "commit-message-validator": {
      "pattern": "(feat|fix|fix-r|refactor)?:",
      "errorMessage": "Invalid commit message"
    }
  },
  "packageManager": "pnpm@8.1.0"
}
